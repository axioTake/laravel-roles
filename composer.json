{
    "name": "axioTake/laravel-roles",
    "description": "Powerful package for handling roles and permissions in Laravel. Supports Laravel 5.3, 5.4, and 5.5+",
    "keywords": ["roles","permissions","acl","levels","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Kenedy",
            "email": "jeremykenedy@gmail.com"
        },
        {
            "name": "axioTake",
            "email": "fun467x@yahoo.de"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*"
    },
    "autoload": {
        "psr-4": {
            "axioTake\\LaravelRoles\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "axioTake\\LaravelRoles\\RolesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
